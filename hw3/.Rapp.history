cacheSolve <- function(x, ...) {#
        ## Return a matrix that is the inverse of 'x'#
        m <- x$getInverse()#
#
## return if inverse is calculated#
        f( !is.null(m) ) {#
          return(m)#
        }#
        ## if already calculated, retrieve the inverse from cache#
        data <- x$get()#
        m <- solve(data) %*% data#
        x$setInverse(m)#
        return(m)#
        }
